    "@types/chrome": "^0.0.140",
    "@types/jest": "^26.0.23",
    "@types/luxon": "^1.26.5",
    "@types/pubsub-js": "^1.8.2",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.26.0",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "parcel-plugin-web-extension": "^1.6.1",
    "typescript": "^4.2.4",
    "web-ext": "^6.0.0"
  },
  "dependencies": {
    "luxon": "^1.27.0",
    "react": "^17.0.1",
