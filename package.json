{
  "name": "productive-cycles-webext",
  "version": "0.8.0",
  "author": "Diego Serrano",
  "license": "MIT",
  "private": true,
  "scripts": {
    "bundle:dev": "webpack --watch --progress --mode=development",
    "bundle:prod": "webpack --mode=production",
    "zip": "web-ext build --source-dir=build --artifacts-dir=dist --overwrite-dest",
    "dev": "npm run bundle:dev",
    "prod": "npm run bundle:prod && npm run zip",
    "start:firefox": "web-ext run --source-dir ./build",
    "start:chrome": "web-ext run --source-dir ./build --target chromium",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src\"/**/*.{ts,tsx}\" --quiet",
    "format": "prettier --write src\"/**/*.{js,jsx,ts,tsx}\""
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/chrome": "^0.0.154",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.12",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.2",
    "style-loader": "^3.2.1",
    "typescript": "^4.3.5",
    "web-ext": "^6.3.0",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.1"
  }
}
